package chapter_4;

import java.util.Scanner;

public class Test_4 {
    public static void main(String[] args)
    {
Scanner sc=new Scanner(System.in);
        System.out.print("Enter you Value :  ");
        int n= sc.nextInt();
        System.out.println("Your Multiplication");
        for (int i=1;i<=10;i++)
        {
            System.out.printf("%d x %d =%d\n",i,n,n*i);
        }
        System.out.println("good, try other number");
    }
}
